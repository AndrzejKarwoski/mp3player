<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="640.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.andrzejkarwoski.controller.ControlPaneController">
    <children>
        <Button id="prev" fx:id="prevButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" HBox.hgrow="NEVER" />
        <Button id="next" fx:id="nextButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" HBox.hgrow="NEVER" />
        <ToggleButton id="playPause" fx:id="playPauseButton" mnemonicParsing="false" prefHeight="75.0" prefWidth="75.0" HBox.hgrow="NEVER" />
        <Slider fx:id="volumeSlider" orientation="VERTICAL" prefHeight="80.0" prefWidth="28.0" HBox.hgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></Slider>
        <Slider fx:id="progressSongSlider" prefHeight="80.0" prefWidth="355.0" HBox.hgrow="ALWAYS">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></Slider>
    </children>
</HBox>
